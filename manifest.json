{
    "name": "newPlugin",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://*/*" ,"https://*/*"
            ],
            "js": [
                "content.js",
                "src/qrcode.js"
            ]
            
        }
    ],
    "browser_action": {
        "default_popup" : "popup.html",
        "default_title": "button"
    },  
    "permissions" : [
        "http://*/*" , "https://*/*"
    ]
}