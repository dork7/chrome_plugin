{
    "name": "QR_code",
    "version": "1.0",
    "manifest_version": 2,
    "web_accessible_resources": ["*.jpg","*.JPG"],
    "icons": { "128" : "QR_image.png"},
    "content_scripts": [
        {
            "matches": [
                "http://*/*" ,"https://*/*"
            ],
            "js": [
                "content.js",
                "src/qrcode.js"
            ]
            
        }
    ],
    "browser_action": {
        "default_popup" : "popup.html",
        "default_title": "button",
        "default_icon":"QR_image.png"
    },  
    "permissions" : [
        "http://*/*" , "https://*/*"
    ]
}